{
  "data_asset_type": "Dataset",
  "expectation_suite_name": "raw_profiles_contract",
  "expectations": [
    {
      "expectation_type": "expect_table_columns_to_match_set",
      "kwargs": {
        "column_set": [
          "user_id",
          "signup_date",
          "country",
          "email_opt_in"
        ]
      },
      "meta": {
        "notes": "Checks that the columns in the table match our contract."
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": { "column": "user_id" },
      "meta": { "contract_field": "user_id" }
    },
    {
      "expectation_type": "expect_column_values_to_be_of_type",
      "kwargs": {
        "column": "signup_date",
        "type_list": ["STRING"]
      },
      "meta": { "contract_field": "signup_date" }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "country",
        "regex": "^[A-Z]{2}$"
      },
      "meta": { "contract_field": "country" }
    }
  ],
  "meta": {
    "great_expectations_version": "0.18.9"
  }
}
